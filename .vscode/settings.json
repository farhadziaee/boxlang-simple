{
    "cflint.enabled": true,
    "cflint.jarPath": "cflint/CFLint-1.5.0-all.jar",
    "cflint.altConfigFile.path": "cflint",
    "cflint.outputDirectory": "cflint",
    "evenBetterToml.formatter.crlf": true,
    "cfml.boxlang.jarpath": "boxlang.jar",
    "cfml.boxlang.miniserverjarpath": "boxlang-miniserver.jar",
    "cfml.boxlang.webPort": 8888,
    "i18n-ally.localesPaths": "./i18n",
    "i18n-ally.sourceLanguage": "en",
    "i18n-ally.keystyle": "flat",
    "i18n-ally.preferredDelimiter": "_",
    "i18n-ally.sortKeys": true,
    "i18n-ally.keepFulfilled": true,
    "i18n-ally.translate.promptSource": true,
    "i18n-ally.pathMatcher": "{locale}.json",
    "i18n-ally.translate.saveAsCandidates": true,
    "i18n-ally.editor.preferEditor": true,
    "i18n-ally.keysInUse": [
        "view.progress_submenu.translated_keys",
        "view.progress_submenu.missing_keys",
        "view.progress_submenu.empty_keys"
    ],
    "i18n-ally.usage.scanningIgnore": [
        "examples"
    ],
    "i18n-ally.regex.usageMatchAppend": [
        "/\\*\\s*i18n\\s*\\*/\\s*['\"`]({key})['\"`]",
        "'{key}'.tr",
    ],
    "i18n-ally.regex.usageMatch": [
        "\"([^\"]+)\"(.tr|.trArgs|.trPlural|.trParams|.trPluralParams)",
        "'([^']+)'(.tr|.trArgs|.trPlural|.trParams|.trPluralParams)",
        "DS\\.tr\\(\"([^\"]+)\"\\)",
        "DS\\.tr\\('([^']+)'\\)",
        "\\$t\\('([^']+)'\\)",
        "\\$t\\(\"([^\"]+)\"\\)",
    ],
    "i18n-ally.enabledFrameworks": [
        "vscode",
        "vue",
        "general",
        "flutter",
        "globalize",
        "custom",
    ],
    "i18n-ally.theme.annotation": "rgba(0, 255, 0, 1)",
    "i18n-ally.theme.annotationBorder": "rgba(0, 150, 150, 1)",
    "i18n-ally.theme.annotationMissing": "#FF5500",
    "i18n-ally.theme.annotationMissingBorder": "#0055FF",
    "i18n-ally.fullReloadOnChanged": true,
    
}